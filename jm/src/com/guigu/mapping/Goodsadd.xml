<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="goodsadd">	
	<insert id="findgoodsadd"  parameterType="java.util.Map">
		insert into c_goods(goodsname,goodstock,unitprice,picture,salestate) values(#{goodsname},#{goodstock},#{unitprice},#{picture},#{salestate})
	</insert>
	
</mapper>
